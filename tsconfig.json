{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@core/*": [
                "src/core/*"
            ],
            "@page/*": [
                "src/page/*"
            ],
            "@fixtures/*": [
                "src/fixtures/*"
            ],
            "@config/*": [
                "sre/config/*"
            ]
        }
    }
}